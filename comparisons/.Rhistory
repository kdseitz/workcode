randomize<-function(x,y){
mat<-matrix(data=NA,nrow=x,ncol=y)
for (i in 1:x){
mat[i,]<-sample(1:y,y,replace=F)
}
return(mat)
}
randomize(15,3)
randomize<-function(x,y,w=NA,z=NA){
mat<-matrix(data=NA,nrow=x,ncol=y)
row.names<-w
col.names<-z
for (i in 1:x){
mat[i,]<-sample(1:y,y,replace=F)
}
return(mat)
}
randomize(15,3)
