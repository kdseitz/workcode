randomize<-function(x,y,w=NA,z=NA){
mat<-matrix(data=NA,nrow=x,ncol=y)
if(is.na(w)){rownames(mat)<-c(1:x)}
else{rownames(mat)<-w}
if(is.na(z)){colnames(mat)<-c(1:y)}
else{colnames(mat)<-z}
for (i in 1:x){
mat[i,]<-sample(1:y,y,replace=F)
}
return(mat)
}
randomize(15,3)
